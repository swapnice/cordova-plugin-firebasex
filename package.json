{
  "name": "cordova-plugin-firebasex",
  "version": "19.0.1",
  "description": "Cordova plugin for Firebase Cloud Messaging (FCM) - messaging only",
  "types": "./types/index.d.ts",
  "author": {
    "name": "Dave Alden"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dpa99c/cordova-plugin-firebase"
  },
  "cordova": {
    "id": "cordova-plugin-firebase",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "keywords": [
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "firebase",
    "fcm",
    "push",
    "notifications",
    "messaging"
  ],
  "scripts": {
    "version": "node ./bump-version.js && git add -A plugin.xml",
    "postversion": "git push && git push --tags && npm publish --verbose",
    "postinstall": "node ./scripts/post_install.js",
    "test": "npm run test:ios && npm run test:android",
    "test:ios": "bash ./test/test-default.sh 9.0.0 ios 5.0.1",
    "test:android": "bash ./test/test-default.sh 9.0.0 android 8.1.0"
  },
  "dependencies": {
    "plist": "^3.0.1",
    "semver": "^7.3.2",
    "xcode": "^3.0.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "cordova-ios": "*"
  }
}
